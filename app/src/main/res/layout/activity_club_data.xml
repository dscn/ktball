<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Refactor.ui.Avtivity.ClubDataActivity">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/imageView9"
        android:src="@drawable/judge_background"
        android:scaleType="fitXY"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:text="金沙江路小学对方充分呢社会财富额"
        android:maxLines="1"
        android:textSize="18dp"
        android:gravity="center"
        android:paddingLeft="36dp"
        android:layout_marginTop="16dp"
        android:textColor="@android:color/white"
        android:id="@+id/club_name_textView"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/imageView15"
        android:layout_toStartOf="@+id/imageView15" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:id="@+id/imageView15"
        android:src="@drawable/qiehuanzhanghao"
        android:onClick="doQuit"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        android:layout_alignTop="@+id/club_name_textView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:id="@+id/textView5"
        android:background="@android:color/darker_gray"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="50dp" />

    <TextView
        android:layout_marginTop="5dp"
        android:layout_width="1dp"
        android:layout_height="40dp"
        android:background="@android:color/darker_gray"
        android:id="@+id/textView6"
        android:layout_alignTop="@+id/textView5"
        android:layout_centerHorizontal="true" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="doXiaoyuan">
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/xiaoyuan_imageView"
                android:src="@drawable/xiaoyuanxinxi_b"
                android:layout_marginRight="6dp"
                android:layout_marginEnd="6dp"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/xiaoyuan_textView"
                android:layout_toStartOf="@+id/xiaoyuan_textView" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:textColor="@color/colorYellow"
                android:text="校园信息"
                android:textSize="18dp"
                android:id="@+id/xiaoyuan_textView"
                android:layout_marginRight="26dp"
                android:layout_marginEnd="26dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="doCaipan">
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/caipan_imageView"
                android:src="@drawable/caipanxintong_a"
                android:layout_marginLeft="23dp"
                android:layout_marginStart="23dp"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:text="裁判系统"
                android:textSize="18dp"
                android:textColor="@color/colorHui"
                android:gravity="center_vertical"
                android:id="@+id/caipan_textView"
                android:layout_marginLeft="9dp"
                android:layout_marginStart="9dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/caipan_imageView"
                android:layout_toEndOf="@+id/caipan_imageView" />
        </RelativeLayout>
    </LinearLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/club_name_textView"
        android:id="@+id/CaiPan_RelativeLayout"
        android:visibility="gone"
        android:layout_centerHorizontal="true"
        android:layout_alignBottom="@+id/textView5">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:orientation="vertical"
            android:layout_marginBottom="80dp"
            android:layout_marginTop="80dp">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:onClick="doJudgment"
                    android:src="@drawable/caipanxuanze"
                    android:id="@+id/ImageView222"
                    android:layout_gravity="center_horizontal" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="人工执裁"
                    android:textColor="@android:color/white"
                    android:textSize="16dp"
                    android:id="@+id/textView307"
                    android:layout_gravity="center_horizontal" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:onClick="doUploading"
                    android:src="@drawable/shipingshangchuan"
                    android:id="@+id/ImageView2"
                    android:layout_gravity="center_horizontal" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="视频上传"
                    android:textColor="@android:color/white"
                    android:textSize="16dp"
                    android:id="@+id/textView37"
                    android:layout_gravity="center_horizontal" />
            </LinearLayout>

        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="35dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:id="@+id/XiaoYuanRelativeLayout"
        android:layout_below="@+id/club_name_textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/textView5">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:onClick="doStudent"
            android:background="@drawable/msg_backg"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:id="@+id/relativeLayout8">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/imageView16"
                android:src="@drawable/student"
                android:layout_marginLeft="8dp"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="学生数"
                android:textSize="17dp"
                android:layout_marginLeft="8dp"
                android:id="@+id/textView8"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imageView16"
                android:layout_toEndOf="@+id/imageView16" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:id="@+id/imageView17"
                android:src="@drawable/gengdou"
                android:layout_marginRight="8dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:layout_marginRight="8dp"
                android:textSize="15dp"
                android:id="@+id/textView10"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/imageView17"
                android:layout_toStartOf="@+id/imageView17" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/msg_backg"
            android:layout_marginTop="12dp"
            android:onClick="doBags"
            android:layout_below="@+id/relativeLayout8"
            android:layout_centerHorizontal="true"
            android:id="@+id/relativeLayout9">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/imageView106"
                android:src="@drawable/qichang"
                android:layout_marginLeft="8dp"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="气场数"
                android:textSize="17dp"
                android:layout_marginLeft="8dp"
                android:id="@+id/textView08"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imageView106"
                android:layout_toEndOf="@+id/imageView106" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:id="@+id/imageView107"
                android:src="@drawable/gengdou"
                android:layout_marginRight="8dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:layout_marginRight="8dp"
                android:textSize="15dp"
                android:id="@+id/textView100"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/imageView107"
                android:layout_toStartOf="@+id/imageView107" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_below="@+id/relativeLayout9"
            android:background="@drawable/msg_backg"
            android:layout_marginTop="12dp"
            android:onClick="doGames"
            android:id="@+id/relativeLayout100"
            android:layout_centerHorizontal="true">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/imageView160"
                android:src="@drawable/saishi"
                android:layout_marginLeft="8dp"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="赛事数"
                android:textSize="17dp"
                android:layout_marginLeft="8dp"
                android:id="@+id/textView008"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imageView160"
                android:layout_toEndOf="@+id/imageView160" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:id="@+id/imageView117"
                android:src="@drawable/gengdou"
                android:layout_marginRight="8dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:layout_marginRight="8dp"
                android:textSize="15dp"
                android:id="@+id/textView110"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/imageView117"
                android:layout_toStartOf="@+id/imageView117" />

        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="15dp"
            android:textColor="@android:color/white"
            android:layout_marginBottom="12dp"
            android:id="@+id/textView11"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true" />

        <Button
            android:layout_width="200dp"
            android:layout_height="45dp"
            android:text="更新数据"
            android:onClick="doUpdata"
            android:textSize="17dp"
            android:layout_marginBottom="6dp"
            android:id="@+id/button"
            android:layout_above="@+id/textView11"
            android:layout_centerHorizontal="true" />


        <ProgressBar
            style="?android:attr/progressBarStyleLarge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/progressBar"
            android:background="@drawable/progress_bg"
            android:indeterminate="false"
            android:indeterminateOnly="false"
            android:minHeight="20dp"
            android:maxHeight="20dp"
            android:padding="5dp"
            android:progress="0"
            android:max="10"
            android:visibility="gone"
            android:progressDrawable="@drawable/progress_horizontal1"
            android:layout_marginBottom="6dp"
            android:layout_above="@+id/textView11"
            android:layout_centerHorizontal="true"/>

        <RelativeLayout
            android:layout_width="110dp"
            android:layout_height="30dp"
            android:visibility="gone"
            android:id="@+id/relativeLayout_hint"
            android:background="@drawable/hint"
            android:layout_above="@+id/button"
            android:layout_alignRight="@+id/button"
            android:layout_alignEnd="@+id/button">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/textView12"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="12dp"
            android:onClick="doClass"
            android:background="@drawable/msg_backg"
            android:layout_below="@+id/relativeLayout100"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/imageView1600"
                android:src="@drawable/class_ico"
                android:layout_marginLeft="8dp"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="班级数"
                android:textSize="17dp"
                android:layout_marginLeft="8dp"
                android:id="@+id/textView0088"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imageView1600"
                android:layout_toEndOf="@+id/imageView1600" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:id="@+id/imageView1177"
                android:src="@drawable/gengdou"
                android:layout_marginRight="8dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_marginRight="8dp"
                android:textSize="15dp"
                android:id="@+id/textView1100"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/imageView1177"
                android:layout_toStartOf="@+id/imageView1177" />
        </RelativeLayout>

    </RelativeLayout>


</RelativeLayout>
